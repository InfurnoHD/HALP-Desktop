@page
@model OperationCHAN.Areas.Lists.Pages.HelpList

@{
    ViewData["Title"] = "Lab Queue";
}
<body>
<div class="navbar d-flex flex-row">
    
    <div class="col-sm" id="queue-title">Lab Queue</div>

    <div class="col-sm d-flex flex-row-reverse" id="form">
        <form class="form-inline" asp-area="Identity" asp-page="/Index" asp-route-returnUrl="@Url.Action("Index", "Home", new {area = ""})">
            <button type="submit" class="btn btn-link text-white text-decoration-none">Settings</button>
        </form>
        
        <form class="form-inline" asp-area="Identity" asp-page="/Index" asp-route-returnUrl="@Url.Action("Index", "Home", new {area = ""})">
            <button type="submit" class="btn btn-link text-white text-decoration-none">Archive</button>
        </form>
    </div>

</div>
<table id="table">
    <tr>
        <th>Nickname</th>
        <th>Description</th>
        <th>Status</th>
    </tr>
    @{
        foreach (var entry in Model.ListItems)
        {
            <tr>
                <td>@Html.DisplayFor(item => entry.Nickname)</td>
                <td>@Html.DisplayFor(item => entry.Description)</td>
                <td>@Html.DisplayFor(item => entry.Status)</td>
            </tr>
        }
    }
</table>
<button id="sendButton" type="submit" class="btn btn-link text-black text-decoration-none">Test</button>
</body>